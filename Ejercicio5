x <- c(125.3,98.2, 201.4,147.3, 145.9, 124.7, 112.2, 120.2, 161.2, 178.9, 159.5, 145.8, 75.1, 151.4, 
       144.2, 125, 198.8, 132.5, 159.6, 110.7)
y <- c(77.9, 76.8, 81.5, 79.8, 78.2, 78.3, 77.5, 77, 80.1, 80.2, 79.9, 79, 76.7, 78.2, 79.5, 78.1,
       81.5, 77, 79, 78.6)

sxx <- sum(x^2)- ((sum(x))^2)/20
sxy <- sum(x*y)-((sum(x)*sum(y))/20)

Beta1 <- sxy/sxx
Beta0 <- mean(y)- (Beta1*mean(x))

yest <- sum((y-(Beta0+Beta1*x))^2)


scuadrada<- yest/18
